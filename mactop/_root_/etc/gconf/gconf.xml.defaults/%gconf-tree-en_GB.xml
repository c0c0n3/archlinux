				<entry name="current_tab">
					<local_schema short_desc="Saves the currently viewed tab">
						<longdesc>0 for the System Info, 1 for the processes list, 2 for the resources and 3 for the disks list</longdesc>
					</local_schema>
				</entry>
				<entry name="view_as">
					<local_schema short_desc="Determines which processes to show by default. 0 is All, 1 is user, and 2 is active">
					</local_schema>
				</entry>
				<entry name="disks_interval">
					<local_schema short_desc="Time in milliseconds between updates of the devices list">
					</local_schema>
				</entry>
				<entry name="show_all_fs">
					<local_schema short_desc="Whether information about all file systems should be displayed">
						<longdesc>Whether to display information about all file systems (including types like &apos;autofs&apos; and &apos;procfs&apos;). Useful for getting a list of all currently mounted file systems.</longdesc>
					</local_schema>
				</entry>
				<entry name="graph_update_interval">
					<local_schema short_desc="Time in milliseconds between updates of the graphs">
					</local_schema>
				</entry>
				<entry name="update_interval">
					<local_schema short_desc="Time in milliseconds between updates of the process view">
					</local_schema>
				</entry>
				<entry name="kill_dialog">
					<local_schema short_desc="Show warning dialogue when killing processes">
					</local_schema>
				</entry>
				<entry name="smooth_refresh">
					<local_schema short_desc="Enable/Disable smooth refresh">
					</local_schema>
				</entry>
				<entry name="solaris_mode">
					<local_schema short_desc="Solaris mode for CPU percentage">
						<longdesc>If TRUE, system-monitor operates in &apos;Solaris mode&apos; where a task&apos;s CPU usage is divided by the total number of CPUs. Otherwise, it operates in &apos;Irix mode&apos;.</longdesc>
					</local_schema>
				</entry>
				<entry name="show_tree">
