				<entry name="smooth_refresh">
					<local_schema short_desc="スムース・リフレッシュの有効/無効">
					</local_schema>
				</entry>
				<entry name="solaris_mode">
					<local_schema short_desc="CPU の使用率を Solaris モードで表示する">
						<longdesc>TRUE にすると &apos;Solaris モード&apos; で動作するようになります。これはタスクの CPU 使用率を CPU の個数で割った値になるモードです。それ以外は &apos;IRIX モード&apos; で動作します。</longdesc>
					</local_schema>
				</entry>
				<entry name="show_tree">
