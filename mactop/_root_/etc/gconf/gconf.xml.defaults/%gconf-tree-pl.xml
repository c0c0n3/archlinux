				<entry name="current_tab">
					<local_schema short_desc="Zapisuje obecnie oglądaną zakładkę">
						<longdesc>0 - informacje o systemie, 1 - lista procesów, 2 - zasoby i 3 - lista dysków</longdesc>
					</local_schema>
				</entry>
				<entry name="view_as">
					<local_schema short_desc="Określa, które procesy będą domyślnie wyświetlane. 0 - wszystkie, 1 - użytkownika i 2 - aktywne">
					</local_schema>
				</entry>
				<entry name="disks_interval">
					<local_schema short_desc="Czas w milisekundach pomiędzy aktualizacjami listy urządzeń">
