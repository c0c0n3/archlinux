<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="procman">
				<entry name="net_out_color">
					<local_schema short_desc="預設網絡圖表（流出）的顏色">
					</local_schema>
				</entry>
				<entry name="net_in_color">
					<local_schema short_desc="預設網絡圖表（流入）的顏色">
					</local_schema>
				</entry>
				<entry name="swap_color">
					<local_schema short_desc="圖表預設表示交換記憶的顏色">
					</local_schema>
				</entry>
				<entry name="mem_color">
					<local_schema short_desc="圖表預設表示記憶體的顏色">
					</local_schema>
				</entry>
				<entry name="cpu_color3">
					<local_schema short_desc="圖表預設表示 CPU 的顏色">
					</local_schema>
				</entry>
				<entry name="cpu_color2">
					<local_schema short_desc="圖表預設表示 CPU 的顏色">
					</local_schema>
				</entry>
				<entry name="cpu_color1">
					<local_schema short_desc="圖表預設表示 CPU 的顏色">
					</local_schema>
				</entry>
				<entry name="cpu_color0">
					<local_schema short_desc="圖表預設表示 CPU 的顏色">
					</local_schema>
				</entry>
				<entry name="current_tab">
					<local_schema short_desc="本數值表示程式下次啟動時顯示的分頁">
						<longdesc>0  為系統資訊，1 為程序清單，2 為資源，3 為磁碟清單</longdesc>
					</local_schema>
				</entry>
				<entry name="view_as">
					<local_schema short_desc="決定顯示哪一類程序。0 表示全部，1 只顯示用家自己的程序，2 只顯示目前仍然活躍的程序。">
					</local_schema>
				</entry>
				<entry name="disks_interval">
					<local_schema short_desc="儲存裝置清單每次更新的時間，以毫秒表示">
					</local_schema>
				</entry>
				<entry name="show_all_fs">
					<local_schema short_desc="是否要顯示關於所有檔案系統的資訊">
						<longdesc>是否要顯示關於所有檔案系統的資訊（這裏包括了像‘autofs’和‘procfs’這種檔案類型）。在取得目前所有掛載的檔案系統清單時會很有用。</longdesc>
					</local_schema>
				</entry>
				<entry name="graph_update_interval">
					<local_schema short_desc="資源圖表每次更新的時間，以毫秒表示">
					</local_schema>
				</entry>
				<entry name="update_interval">
					<local_schema short_desc="程序清單每次更新的時間，以毫秒表示">
					</local_schema>
				</entry>
				<entry name="kill_dialog">
					<local_schema short_desc="準備強行終止程序時顯示警告訊息對話方塊">
					</local_schema>
				</entry>
				<entry name="smooth_refresh">
					<local_schema short_desc="啟用/停用平滑更新">
					</local_schema>
				</entry>
				<entry name="solaris_mode">
					<local_schema short_desc="CPU 百分比使用 Solaris 模式">
						<longdesc>如設為 TRUE，system-monitor 會以「Solaris 模式」運行，即每個工作的 CPU 使用量會除以總 CPU 數來均分。否則它會以「Irix 模式」運行。</longdesc>
					</local_schema>
				</entry>
				<entry name="show_tree">
					<local_schema short_desc="以樹狀方式顯示程序間的相根據性">
					</local_schema>
				</entry>
				<entry name="height">
					<local_schema short_desc="主視窗高度">
					</local_schema>
				</entry>
				<entry name="width">
					<local_schema short_desc="主視窗闊度">
					</local_schema>
				</entry>
				<dir name="memmapstree">
					<entry name="sort_order">
						<local_schema short_desc="排列程序的次序">
						</local_schema>
					</entry>
					<entry name="sort_col">
						<local_schema short_desc="程序根據哪一欄排序">
						</local_schema>
					</entry>
				</dir>
